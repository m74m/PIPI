\par 
\par float sumkva(float *p, int n) \{
\par   float retval=0;
\par   for (;n-- > 0; p++)
\par     retval += *p * *p;
\par   return retval;
\par \}
\par 
\par void funb(float *p, int m, int n, int maxred) \{
\par   float skred;
\par   int red, stup;
\par 
\par   for (red=0; red<m; red++) \{
\par     skred=sumkva(p+red*maxred, n);
\par     for (stup=0; stup<n; stup++)
\par        p[red*maxred+stup] /= pow (skred, 1./3);
\par   \}
\par \}
\par 
\par void main () \{
\par   int m, n, i, j;
\par   float p[100][10];
\par 
\par   do \{
\par     printf ("Upisite broj redaka (<=100) i stupaca (<=10) polja: ");
\par     scanf ("%d %d", &m, &n);
\par   \} while (m < 1 || m > 100 || n < 1 || n > 10);
\par 
\par   for (i = 0; i < m; i++)
\par     for (j = 0; j < n; j++) \{
\par        printf ("Polje[%d][%d]= ", i+1, j+1);
\par        scanf ("%f", &p[i][j]);
\par     \}
\par 
\par   funb (&p[0][0], m, n, 10);
\par   printf ("   ");
\par   for (i = 0; i < n; i++) printf("%7d", i+1);
\par   printf ("\\n");
\par   printf ("---");
\par   for (i = 0; i < n; i++) printf("-------");
\par   printf ("\\n");
\par   for (i = 0; i < m; i++) \{
\par      printf ("%3d", i+1);
\par      for (j = 0; j < n; j++) printf("%7.1f", p[i][j]);
\par      printf("\\n");
\par   \}
\par \}
\par }\pard\plain \ql \li0\ri0\nowidctlpar{\*\pn \pnlvlcont\ilvl12\ls0\pnrnot0\pndec }\faauto\ilvl12\rin0\lin0\itap0\pararsid2892760 \fs24\lang1033\langfe1033\cgrid\langnp1033\langfenp1033 }}\pard\plain \ql \li0\ri0\nowidctlpar{\*\pn 
