\\\\program\\\\Z\\\\1.c" \\c AnsiText }}{\fldrslt {\insrsid2892760 #include <stdio.h>
\par #include <string.h>
\par #include "lib.h"
\par 
\par long niz_u_broj(char * cp) \{
\par   long retval;
\par   int n;
\par 
\par   for (retval = 0L, n = 0; *cp; n++, cp++) \{
\par     if (*cp < '0' || *cp > '9')  \{
\par       fatal ("Nadjen znak koji nije broj.");
\par     \}
\par     if (n == 9) \{
\par       fatal ("Predugacak niz brojeva.");
\par     \}
\par     retval = 10 * retval + *cp - '0';
\par   \}
\par   return retval;
\par \}
\par 
\par void main () \{
\par   char niz[256], *cp;
\par 
\par   while (1) \{
\par     gets(niz);
\par     cp = strchr(niz, ' ');
\par     if (cp == NULL) fatal ("U nizu nema praznine.");
\par     *cp = '\\0';
\par     switch (*(cp+1)) \{
\par       case '+': 
\par         printf("Rezultat je:%ld\\n",
\par                niz_u_broj(niz) + niz_u_broj(cp+3));
\par         break;
\par       case '-': 
\par         printf("Rezultat je:%ld\\n",
\par                niz_u_broj(niz) - niz_u_broj(cp+3));
\par         break;
\par       case '*': 
\par         printf("Rezultat je:%ld\\n",
\par                niz_u_broj(niz) * niz_u_broj(cp+3));
\par         break;
\par       case '/': 
\par         printf("Rezultat je:%ld\\n",
\par                niz_u_broj(niz) / niz_u_broj(cp+3));
\par         break;
\par       default: 
\par         exit(1);
\par     \}
\par   \}
\par \}
\par }\pard\plain \ql \li0\ri0\nowidctlpar{\*\pn \pnlvlcont\ilvl12\ls0\pnrnot0\pndec }\faauto\ilvl12\rin0\lin0\itap0\pararsid2892760 \fs24\lang1033\langfe1033\cgrid\langnp1033\langfenp1033 }}\pard\plain \ql \li0\ri0\nowidctlpar{\*\pn 
