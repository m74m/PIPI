\par     printf("\\n");
\par   \}
\par 
\par   printf("nIspis drugog polja:\\n");
\par   for (i = 0; i < brred; i++) \{
\par     for(j = 0; j < brst; j++)
\par       printf("%5.2f", polje2[i][j]);
\par     printf("\\n");
\par   \}
\par   if (slicna_polja(polje1, polje2, brred, brst, MAXS))
\par     printf("Polja su slicna.\\n");
\par   else
\par     printf("Polja su razlicita.\\n");
